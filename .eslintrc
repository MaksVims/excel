{
  "parser": "babel-eslint",
  "env": {
    "es6": true,
    "node": true,
    "browser": true,
    "jest/globals": true
  },
  "rules": {
    "semi": "off",
    "require-jsdoc": "off",
    "linebreak-style": 0,
    "arrow-parens": "off",
    "comma-dangle": "off"
  },
  "extends": ["eslint:recommended", "google"],
  "plugins": ["jest"]
}